@model Team
<div class="container">
    <form method="post" enctype="multipart/form-data">
        <div class="row">
            <div class="mb-3 col-md-4">
                <label class="form-label" asp-for="FUllName">FullName</label>
                <input class="form-control" asp-for="FUllName">

                <span asp-validation-for="FUllName" style="color:red"></span>
            </div>
            <div class="mb-3 col-md-4">
                <label class="form-label" asp-for="Position">Position</label>
                <input class="form-control" asp-for="Position">
                <span asp-validation-for="Position" style="color:red"></span>
            </div>
            <div class="mb-3 col-md-4">
                <label class="form-label" asp-for="Description">Description</label>
                <input class="form-control" asp-for="Description">
                <span asp-validation-for="Description" style="color:red"></span>
            </div>
            <div class="mb-3 col-md-4">
                <label class="form-label" asp-for="ImgFile">ImgFile</label>
                <input class="form-control" asp-for="ImgFile">
                <span asp-validation-for="ImgFile" style="color:red"></span>
            </div>
        </div>

        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>